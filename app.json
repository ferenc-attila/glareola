{
  "expo": {
    "name": "Glareola",
    "slug": "glareola",
    "version": "0.0.4",
    "githubUrl": "https://github.com/ferenc-attila/glareola",
    "icon": "./assets/images/glareola_portrait.png",
    "userInterfaceStyle": "dark",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/glareola_pratincola.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/glareola_portrait.png",
        "backgroundColor": "#ffffff"
      },
      "package": "hu.delheves.glareola"
    },
    "web": {
      "favicon": "./assets/images/glareola_portrait.png"
    },
    "extra": {
      "eas": {
        "projectId": "d3975595-4258-4e8d-8cd8-b59c09d0e8dd"
      }
    },
    "owner": "expopuma",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}
